        <activity android:name=".dagger2.Dagger2Activity">
        </activity>
        <activity android:name=".arouter.ARouterActivity">
        </activity>
        <activity android:name=".leakcanary.LeakCanaryActivity">
        </activity>
        <activity android:name=".tinker.TinkerActivity">
        </activity>
        <activity android:name=".sophix.SophixActivity">
        </activity>
        <activity android:name=".glide.GlideActivity">
        </activity>
